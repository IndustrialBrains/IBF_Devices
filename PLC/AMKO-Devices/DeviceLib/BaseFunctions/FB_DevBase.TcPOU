      </Implementation>
    </Method>
    <Method Name="CmdSafeStop" Id="{dec1b3af-006d-0b48-0f79-6a1440a93d4c}">
      <Declaration><![CDATA[METHOD PUBLIC CmdSafeStop : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[// check if device is safe
bIsSafe 	:= NOT bEnable ;
CmdSafeStop := bIsSafe ;]]></ST>
      </Implementation>
    </Method>
