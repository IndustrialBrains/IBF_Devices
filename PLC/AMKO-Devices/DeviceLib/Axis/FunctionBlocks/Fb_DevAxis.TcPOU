]]></Declaration>
      <Implementation>
        <ST><![CDATA[// ===== Execute Jog function =====
	// velocity limiting
		IF bCmdSafeSpeed THEN 
			fbMcJog.Velocity := MIN(fSafeVeloLimit, fbMcJog.Velocity); 
		ELSIF bCmdLimitVelocity THEN 
			fbMcJog.Velocity := MIN(fCmdVelocityLimit, fbMcJog.Velocity); 
		END_IF
